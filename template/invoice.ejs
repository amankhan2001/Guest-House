<!DOCTYPE html>
<html>

<head>
    <style>
        table {
            border-collapse: collapse;
        }

        table,
        th,
        td {
            border: 1px solid black;
        }
    </style>
</head>

<body>
    <div style="width: 85%; margin: 0 7.5%;">
        <div style="text-align: center; padding-left: 50px;">
            <h3>Report</h5>
                <table>
                    <% for (let res of payload){%>
                    <% Object.entries(res).forEach(([key, value]) =>{%>
                    <tr>
                        <td><%= key %></td>
                        <td><%= value %></td>
                    </tr>
                    <%  });%>
                    <%  };%>
                </table>
        </div>
    </div>
</body>

</html>