<!DOCTYPE html>
<html>

<head>
    <style>
        table {
            border-collapse: collapse;
        }

        table,
        th,
        td {
            border: 1px solid black;
        }
    </style>
</head>

<body>
    <div style="align-items: center;margin-top: 10%;">
        <h3>Dependency check between Quality <%= payload.currentServer %> and <%=  payload.nextServer %></h3>
    </div>
    <div style="width: 85%; margin: 0 7.5%;">
        <div>
            <div>
                <div style="text-align: left;">
                    <span>Change Request Number:</span> <span> <%= payload.crTicketNumber %></span>
                </div>
                <div style="text-align: right;">
                    <span>Date : </span> <span> <%= payload.executionDate %></span>
                </div>
            </div>
        </div>
        <div>
            <div style="text-align: left;">
                <span>Description : </span> <span> <%= payload.description %></span>
            </div>
            <div style="text-align: right;">
                <span>Time : </span> <span> <%= payload.time %></span>
            </div>
        </div>
        <div>
            <div style="text-align: left;">
                <span>Generated By : </span> <span> <%= payload.userName %></span>
            </div>
        </div>
    </div>
    <div style="width: 85%; margin: 0 7.5%;">
        <div style="text-align: center; padding-left: 50px;">
            <h3>Report</h5>
                <table>
                    <% for (let res of payload.pdfData){%>
                    <tr>
                        <% Object.entries(res).forEach(([key, value]) =>{%>

                        <th><%= key %></th>

                        <%  });%>
                    </tr>
                    <% break %>
                    <%  };%>
                    <% for (let res of payload.pdfData){%>
                    <tr>
                        <% Object.entries(res).forEach(([key, value]) =>{%>

                        <td><%= value %></td>

                        <%  });%>
                    </tr>
                    <%  };%>
                </table>
        </div>
    </div>
</body>

</html>